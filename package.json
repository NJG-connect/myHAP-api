{
  "name": "api",
  "version": "1.0.1",
  "author": "jeremynoh",
  "description": "API of fmdc",
  "main": "dist/src/index.js",
  "bin": "dist/src/index.js",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc ; npx pkg -t linux,macos,win .",
    "dev": "tsc && NODE_ENV=development ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "NODE_ENV=local npx nodemon ./src/index.ts",
    "start:dev": "NODE_ENV=development npx nodemon ./src/index.ts",
    "start:prod": "NODE_ENV=prod node dist/src/index.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.42",
    "@types/node-fetch": "^2.6.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "dotenv-cli": "^6.0.0",
    "nodemon": "^2.0.16",
    "pkg": "^5.7.0",
    "prisma": "^4.0.0",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tsc-hooks": "^1.1.1"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "@types/cors": "^2.8.12",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "2",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "targets": [
      "node14-linux-arm64"
    ],
    "assets": [
      "./dist/src/swagger/index.html",
      "./dist/src/swagger/swagger.json"
    ],
    "outputPath": "apps"
  }
}
